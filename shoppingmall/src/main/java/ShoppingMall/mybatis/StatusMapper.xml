<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Status">
	
	<sql id="columns">
		status_id, status_name
	</sql>
	
	<select id="selectAll" resultType="Status">
		SELECT <include refid="columns"/> FROM status
	</select>
	
	<select id="select" parameterType="int" resultType="Status">
		SELECT <include refid="columns"/> FROM status WHERE status_id = #{status_id}
	</select>
	
</mapper>